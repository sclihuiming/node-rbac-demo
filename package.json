{
  "name": "node-rbac-demo",
  "version": "1.0.0",
  "description": "基于rbac的一个权限系统",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-node-rbac-demo",
    "stop": "egg-scripts stop --title=egg-server-node-rbac-demo",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean"
  },
  "dependencies": {
    "acl": "^0.4.11",
    "egg": "^2.6.1",
    "egg-mongoose": "^3.1.1",
    "egg-scripts": "^2.6.0",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@types/acl": "^0.4.36",
    "@types/mocha": "^2.2.40",
    "@types/mongoose": "^5.3.24",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "tslib": "^1.9.0",
    "tslint": "^5.0.0",
    "tslint-config-egg": "^1.0.0",
    "typescript": "^3.0.0",
    "uninstall": "0.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "lihuiming",
  "license": "MIT"
}
